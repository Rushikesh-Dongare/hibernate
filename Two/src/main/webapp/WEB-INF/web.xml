<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <servlet>
  	<servlet-name>add</servlet-name>
  	<servlet-class>com.Rushikesh.Add</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>square</servlet-name>
  	<servlet-class>com.Rushikesh.Square</servlet-class> 	
  	<init-param>
  		<param-name>name</param-name>
  		<param-value>Rushikesh</param-value>
  	</init-param>
  	<init-param>
  		<param-name>phone</param-name>
  		<param-value>Samsung</param-value>
  	</init-param>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>add</servlet-name>
  	<url-pattern>/add</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>square</servlet-name>
  	<url-pattern>/square</url-pattern>
  </servlet-mapping>
</web-app>